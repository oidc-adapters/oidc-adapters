{
  "name": "@oidc-adapters/keycloak",
  "description": "Keycloak adapters",
  "version": "0.0.0-development",
  "license": "Apache-2.0",
  "author": {
    "name": "RÃ©mi Alvergnat",
    "email": "toilal.dev@gmail.com",
    "url": "https://github.com/Toilal"
  },
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "clean": "rimraf dist/ server/",
    "build": "yarn build:types && yarn build:swc",
    "build:swc": "swc ./src --source-maps inline --out-dir ./dist/esm",
    "build:types": "tsc -p tsconfig.types.json --emitDeclarationOnly",
    "dev": "concurrently 'yarn dev:swc' 'yarn dev:types'",
    "dev:swc": "swc ./src --source-maps inline --out-dir ./dist/esm -w",
    "dev:types": "tsc -p tsconfig.types.json --emitDeclarationOnly --watch --preserveWatchOutput",
    "lint": "eslint . --fix",
    "lint:ci": "eslint .",
    "test": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest --group=-selenium --group=-timeout",
    "test:ci": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest --group=-selenium --group=-timeout",
    "test:all": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest --runInBand",
    "test:timeout": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest --group=timeout",
    "test:selenium": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest --group=selenium --runInBand",
    "typecheck": "tsc -p tsconfig.types.json --noEmit",
    "typecheck:ci": "tsc -p tsconfig.types.json --noEmit",
    "app:start": "tsx ./scripts/start-app.ts",
    "server:start": "tsx ./scripts/start-server.ts --import-realm"
  },
  "dependencies": {
    "@oidc-adapters/core": "workspace:*",
    "jwk-to-pem": "^2.0.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4",
    "remove": "^0.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@keycloak/keycloak-admin-client": "^20.0.2",
    "@octokit/rest": "^19.0.5",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "@swc/jest": "^0.2.24",
    "@types/chromedriver": "^81.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.5",
    "@types/fs-extra": "^11.0.1",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/minimist": "^1.2.2",
    "@types/mustache-express": "^1.2.2",
    "@types/node-fetch": "^2.6.2",
    "@types/passport-jwt": "^3.0.8",
    "@types/selenium-webdriver": "^4.1.10",
    "@types/server-destroy": "^1.0.1",
    "@types/supertest": "^2.0.12",
    "@types/tar-fs": "^2.0.1",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "chokidar": "^3.5.3",
    "chromedriver": "^108.0.0",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fetch-mock": "^9.11.0",
    "fs-extra": "^11.1.0",
    "gunzip-maybe": "^1.4.2",
    "jest": "^29.3.1",
    "jest-mock": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-runner-groups": "^2.2.0",
    "keycloak-request-token": "^0.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.7",
    "mustache-express": "^1.3.2",
    "oidc-client-ts": "^2.2.0",
    "pify": "^6.1.0",
    "rimraf": "^3.0.2",
    "rsa-compat": "^2.0.8",
    "selenium-webdriver": "^4.7.1",
    "server-destroy": "^1.0.1",
    "supertest": "^6.3.3",
    "tar-fs": "^2.1.1",
    "ts-jest": "^29.0.3",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "oidc",
    "openid",
    "passport"
  ]
}
