{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "importsNotUsedAsValues": "remove",
    "types": [
      "node",
      "jest"
    ]
  }
}
